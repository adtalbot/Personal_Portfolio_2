<% content_for(:title, "Project | Personal Portfolio") %>

<%= render 'layouts/errors', :object => @project %>

<%= form_for @project do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
<%= f.submit %>
<% end %>

<p><%= link_to "Return to Projects", projects_path %></p>
